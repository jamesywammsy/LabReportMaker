	@Override
	public void actionPerformed(ActionEvent e) {

		title = title_.getText();

		if (title_.getText().equals("")) {
			title = "Unnamed Table";
		}
		table.RefreshCompleted(index, tabDone);

		if (e.getSource() == return_) {

			HideWindow();
			table.ShowWindow();
			
			row = row_.getText();
			col = col_.getText();
			
			data[0][0] = r1c1.getText();
			data[0][1] = r1c2.getText();
			data[0][2] = r1c3.getText();
			data[0][3] = r1c4.getText();
			data[0][4] = r1c5.getText();
			data[0][5] = r1c6.getText();
			data[0][6] = r1c7.getText();
			data[0][7] = r1c8.getText();

			data[1][0] = r2c1.getText();
			data[1][1] = r2c2.getText();
			data[1][2] = r2c3.getText();
			data[1][3] = r2c4.getText();
			data[1][4] = r2c5.getText();
			data[1][5] = r2c6.getText();
			data[1][6] = r2c7.getText();
			data[1][7] = r2c8.getText();

			data[2][0] = r3c1.getText();
			data[2][1] = r3c2.getText();
			data[2][2] = r3c3.getText();
			data[2][3] = r3c4.getText();
			data[2][4] = r3c5.getText();
			data[2][5] = r3c6.getText();
			data[2][6] = r3c7.getText();
			data[2][7] = r3c8.getText();

			data[3][0] = r4c1.getText();
			data[3][1] = r4c2.getText();
			data[3][2] = r4c3.getText();
			data[3][3] = r4c4.getText();
			data[3][4] = r4c5.getText();
			data[3][5] = r4c6.getText();
			data[3][6] = r4c7.getText();
			data[3][7] = r4c8.getText();

			data[4][0] = r5c1.getText();
			data[4][1] = r5c2.getText();
			data[4][2] = r5c3.getText();
			data[4][3] = r5c4.getText();
			data[4][4] = r5c5.getText();
			data[4][5] = r5c6.getText();
			data[4][6] = r5c7.getText();
			data[4][7] = r5c8.getText();

			data[5][0] = r6c1.getText();
			data[5][1] = r6c2.getText();
			data[5][2] = r6c3.getText();
			data[5][3] = r6c4.getText();
			data[5][4] = r6c5.getText();
			data[5][5] = r6c6.getText();
			data[5][6] = r6c7.getText();
			data[5][7] = r6c8.getText();

			data[6][0] = r7c1.getText();
			data[6][1] = r7c2.getText();
			data[6][2] = r7c3.getText();
			data[6][3] = r7c4.getText();
			data[6][4] = r7c5.getText();
			data[6][5] = r7c6.getText();
			data[6][6] = r7c7.getText();
			data[6][7] = r7c8.getText();

			data[7][0] = r8c1.getText();
			data[7][1] = r8c2.getText();
			data[7][2] = r8c3.getText();
			data[7][3] = r8c4.getText();
			data[7][4] = r8c5.getText();
			data[7][5] = r8c6.getText();
			data[7][6] = r8c7.getText();
			data[7][7] = r8c8.getText();

			data[8][0] = r9c1.getText();
			data[8][1] = r9c2.getText();
			data[8][2] = r9c3.getText();
			data[8][3] = r9c4.getText();
			data[8][4] = r9c5.getText();
			data[8][5] = r9c6.getText();
			data[8][6] = r9c7.getText();
			data[8][7] = r9c8.getText();

			data[9][0] = r10c1.getText();
			data[9][1] = r10c2.getText();
			data[9][2] = r10c3.getText();
			data[9][3] = r10c4.getText();
			data[9][4] = r10c5.getText();
			data[9][5] = r10c6.getText();
			data[9][6] = r10c7.getText();
			data[9][7] = r10c8.getText();

		}

		else if (e.getSource() == markAsDone_) {

			tabDone = !tabDone;

			if (tabDone) {
				markAsDone_.setText("Mark as Undone");
			}

			else {
				markAsDone_.setText("Mark as Done");
			}

		}

		else if (e.getSource() == delete_) {

			HideWindow();

			if (index == 0) {
				table.table1 = null;
				table.table1_.setText("+");
			}

			else if (index == 1) {
				table.table2 = null;
				table.table2_.setText("+");
			}

			else if (index == 2) {
				table.table3 = null;
				table.table3_.setText("+");
			}

			else if (index == 3) {
				table.table4 = null;
				table.table4_.setText("+");
			}

			else if (index == 4) {
				table.table5 = null;
				table.table5_.setText("+");
			}

			else if (index == 5) {
				table.table6 = null;
				table.table6_.setText("+");
			}

			else if (index == 6) {
				table.table7 = null;
				table.table7_.setText("+");
			}

			else if (index == 7) {
				table.table8 = null;
				table.table8_.setText("+");
			}

			table.ShowWindow();

		}

		repaint();
	}

	public void HideWindow() {
		setVisible(false);
	}

	public void ShowWindow() {
		setVisible(true);
	}
}
